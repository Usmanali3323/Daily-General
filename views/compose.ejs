<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
<label>Title</label>
<input type="text" class="form-control" placeholder="Title" name="postTitle" aria-label="Username" aria-describedby="addon-wrapping">
<label class="post">Post</label>
<textarea class="form-control" placeholder="Post here" id="floatingTextarea2" name="postBody" style="height: 100px"></textarea>
<button type="submit" class="btn btn-primary compose-button" >Publish</button>
</form>
<%- include("partials/footer") -%>